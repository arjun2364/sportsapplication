
@{
    ViewData["Title"] = "Confirm";
}

@model Sports_Application.Models.Result

<h1>Confirm</h1>
<div>
    <p>Do you want to delete this athelete from this test?</p>
</div>
<a class="btn btn-danger" asp-controller="Result" asp-action="DeleteResult" asp-route-Id="@Model.Id" asp-route-TestId="@Model.TestId">Yes</a>
<a class="btn btn-primary" asp-controller="Result" asp-action="ViewResult" asp-route-TestId="@Model.TestId">No</a>

